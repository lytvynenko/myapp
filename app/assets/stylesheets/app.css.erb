@font-face {
  font-family: 'Lato';
  font-style: normal;
  font-weight: 400;
  src: url(http://themes.googleusercontent.com/static/fonts/lato/v6/9k-RPmcnxYEPm8CNFsH2gg.woff) format('woff');
}

@font-face {
  font-family: 'Gentium Book Basic';
  font-style: normal;
  font-weight: 400;
  src: url(http://themes.googleusercontent.com/static/fonts/gentiumbookbasic/v3/IRFxB2matTxrjZt6a3FUnsiUQA3_wO-qQTJfqhvSKS0.woff) format('woff');
}

@font-face {
  font-family: 'Bad Script';
  font-style: normal;
  font-weight: 400;
  src: url(http://themes.googleusercontent.com/static/fonts/badscript/v2/rL_b2ND61EQmMOJ8CRr1fhsxEYwM7FgeyaSgU71cLG0.woff) format('woff');
}

.navbar .brand {
	margin-left: -10px;
}
#logo {
	padding:2px 10px 0 0;
}

#logo img {
	height:55px;
}

#slogan {
	padding:15px 10px 0 20px;
}

#slogan img {
	height:40px;
}

.navbar-inner {
	background: url(<%= asset_path 'low_contrast_linen.png' %>);
	border-bottom: 2px solid #999;
	filter:none;
}

/*.navbar-fixed-top .navbar-inner, .navbar-static-top .navbar-inner {
	border-width: 0 0 0px;

}*/
.navbar .nav > li > a {
	color:white;
	text-shadow:none;
}

.nav-collapse {
	text-align: center;

}

#main-container {
box-shadow: 6px 0px 10px -4px #333, -6px 0px 10px -4px #333;
margin-top:50px;
padding:10px;
background-color: white;

}

.sequence-canvas {
	list-style: none;
}

body {
	background-image: url(<%= asset_path 'grey_wash_wall.png' %>); background-position: initial initial; background-repeat: initial initial;
/*	background-image: url(http://subtlepatterns.com/patterns/px_by_Gre3g.png); background-position: initial initial; background-repeat: initial initial;
background-image: url(http://subtlepatterns.com/patterns/green-fibers.png); background-position: initial initial; background-repeat: initial initial;
background-image: url(http://subtlepatterns.com/patterns/fabric_plaid.png); background-position: initial initial; background-repeat: initial initial;

*/

}


#wrap {
	min-height: 100%;
        height: auto !important;
        height: 100%;
        /* Negative indent footer by it's height */
        margin: 0 auto;
}

#footer {
	border-top: 1px solid #999;
	background-image: url(<%= asset_path 'low_contrast_linen.png' %>); background-position: initial initial; background-repeat: initial initial;
	color:white;
}

footer { margin-top: 20px; }

.nav-list > li > a, .nav-list .nav-header {
	background: transparent;
	color:white;
	text-shadow: none;
	filter:none;
}

.nav > li > a:hover, .nav > li > a:focus {
	background: transparent;
	color:#bf0000;

}

.nav-list > .active > a, .nav-list > .active > a:hover, .nav-list > .active > a:focus  {
	background: transparent;
	color:#999;
}

.carousel-caption {
  
  background: #999;
  background: rgba(0, 0, 0, 0.6);
  font-family: 'Lato', sans-serif;
}


.carousel-caption h1 {
	color: white;
	font-size:2em;
}


#main-img {
	height: 400px;
	min-height: 400px;
  background: url(<%= asset_path 'heart-bg.png' %>) no-repeat center center;
        -webkit-background-size: cover;
        -moz-background-size: cover;
        -o-background-size: cover;
        background-size: cover;
        filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../img/couple-blured.png', sizingMethod='scale');
    -ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../img/couple-blured.png', sizingMethod='scale')";
    zoom:1;
    vertical-align: middle;
    margin-top:0px;

}

#main-img-text {
	font-family: 'Bad Script', cursive;
	font-size: 1.5em;
	color:#fefefe;
	background:url(<%= asset_path 'black-50.png' %>);
	border:2px solid #bf0000;
	text-align: center;
	vertical-align: middle;
	margin-top:9em; 
	padding: 10px;
	line-height: 1.5em;
}

#main-text {
  font-family: 'Lato', sans-serif;
  font-size: 1.4em;	
  color:#333;

  background: url(<%= asset_path 'couple-blured.png' %>) no-repeat center center;
        -webkit-background-size: cover;
        -moz-background-size: cover;
        -o-background-size: cover;
        background-size: cover;
        filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../img/couple-blured.png', sizingMethod='scale');
    -ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../img/couple-blured.png', sizingMethod='scale')";
    zoom:1;
}   

#main-text p {
	text-align: center;
	line-height: 1.3em;
}
/*#main-text-text {
	background: url(../img/white-50.png);
	padding: 40px 40px 40px 50px;
	margin-right: -20px;
}     
*/
#main-mission {
	font-family: 'Gentium Book Basic', serif;
}

#main-mission h5,#main-mission h3{
	font-family: 'Lato', sans-serif;
}

#main-intro {
color:#333;
font-size: larger;
font-family: 'Gentium Book Basic', sans-serif;
background-image: url(<%= asset_path 'bedge_grunge.png' %>); background-position: initial initial; background-repeat: initial initial;


}



#main-intro h2 {
	font-family: 'Lato', sans-serif;
	text-transform: uppercase;
	font-weight: bold;
	color:#bf0000;
}

#main-intro .span4 {
	padding:15px;
}

#main-intro .span4 p{
	margin-top:10px;
}

#main-intro img,#main-process img {
	height:100px;
}
#main-process {
padding-top: 10px;
margin-bottom: -10px;
color:#333;
font-size: larger;
background-image: url(<%= asset_path 'bedge_grunge.png' %>); background-position: initial initial; background-repeat: initial initial;

}

.divider {

	height: 20px;
	min-height: 20px;

}
#charity-search-wrap {
	min-height:400px;
}
#charityCarousel > .carousel-caption{
	font-size: 0.5em;
}

.accordion-toggle {
	background:#dedede; 
}

#lists {
	height:360px;
	max-height: 360px;
	overflow-y:auto;
	overflow-x:hidden;

}

.inner {
	background-color: #deeede;
}

#search-results {
	height:320px;
	max-height: 320px;
	overflow-y:auto;
	overflow-x:hidden;
	font-size: 0.9em;
}

#search-results .active {
  border-color: rgba(82, 168, 236, 0.8);
  outline: 0;
  outline: thin dotted \9;
  /* IE6-9 */

  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6);
  -moz-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6);
  box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6);

}

