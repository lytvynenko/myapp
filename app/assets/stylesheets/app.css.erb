.accordion-toggle {
	text-decoration: none;
	color:#333;
}
.accordion-toggle:hover {
	text-decoration: none;
	color:#333;
	background: #ccc;	
}


.charity-desc{
	background: #ffc;
}

/* - RESPONSIVE STUFF - */
/* Large desktop */
@media (min-width: 1200px) {

	.container {
	width:1170px;
	}
	#myCarousel {
	margin-left:20px; 
	margin-right:-10px; 
	}

	.with-bg {
	margin-left: -10px;
	margin-right: -10px;
	margin-top: -20px;
	}

}
 
 @media (min-width: 979px) and (max-width: 1200px) { 
#myCarousel {
    margin-left: 10px;
    margin-right: -10px;
}

.with-bg {
	margin-left: -10px;
	margin-right: -10px;
	margin-top: -20px;	
}

#slogan img{
	display: none;
}
#main-img-text {
	font-size: 1.4em;
}
 }
/* Portrait tablet to landscape and desktop */
@media (min-width: 768px) and (max-width: 979px) { 
	#myCarousel {
	margin-left:10px; 
	margin-right:-10px; 

	}

	.with-bg {
	margin-left: -10px;
	margin-right: -10px;
	margin-top: -20px;	
}
	

	#main-container {
	margin-top:-20px;
	padding-top:0px; 
	}

	#main-img-text {
	font-size: 1.3em;
}

}
 
/* Landscape phone to portrait tablet */
@media (max-width: 767px) {

#myCarousel {
	margin-left:10px; 
	margin-right:-10px; 

	}

	.with-bg {
	margin-left: -10px;
	margin-right: -10px;
	
	}

	#main-container {
	margin-top:-20px;
	padding-top:0px; 
	}

		#slogan img{
	height: 30px;
}

footer {margin: 0 -10px;}

}
 
/* Landscape phones and down */
@media (max-width: 480px) {

#main-container {
	margin-top:-20px;
	padding-top:0px; 
	}

	#main-container {
	margin-top:-20px;
	padding-top:0px; 
	}

.navbar .brand {
	margin-left: 2px;
}
	#slogan {
	display: none;
}

#main-img {
	height: 200px;
	min-height: 200px;
}
	#main-img-text {
	margin-top: 0px;
	font-size: 1.25em;
}

.accordion-toggle {
	font-size: 0.8em;
	white-space: nowrap;
  	overflow: hidden;
}

}
